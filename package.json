{
  "name": "officestore",
  "version": "2.0.0",
  "description": "OfficeStore - Pantry & Office Supplies Management System (Single Server Architecture)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run cleanup:js && cross-env NODE_ENV=development tsx server/index.ts",
    "build": "npm run cleanup:js && npm run type-check && vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "cleanup:js": "node DEV-Files/development/utilities/cleanup-js-files.cjs",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:generate": "drizzle-kit generate --config ./server/drizzle.config.ts",
    "db:push": "drizzle-kit push --config ./server/drizzle.config.ts --yes",
    "db:migrate": "drizzle-kit migrate --config ./server/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./server/drizzle.config.ts",
    "migrate:end-users": "tsx server/scripts/migrate-end-users-to-users.ts",
    "migrate:end-users:dry": "tsx server/scripts/migrate-end-users-to-users.ts --dry-run",
    "migrate:end-user-sessions": "tsx server/scripts/migrate-end-user-sessions.ts",
    "migrate:end-user-sessions:dry": "tsx server/scripts/migrate-end-user-sessions.ts --dry-run",
    "migrate:end-user-access": "tsx server/scripts/migrate-end-user-access.ts",
    "migrate:end-user-access:dry": "tsx server/scripts/migrate-end-user-access.ts --dry-run"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-popover": "^1.1.4",
    "@tanstack/react-query": "^5.90.2",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.33.0",
    "express": "^5.0.0",
    "express-rate-limit": "^7.4.0",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.427.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.30.1",
    "reactflow": "^11.11.4",
    "redis": "^4.7.0",
    "uuid": "^13.0.0",
    "vite": "^5.4.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.16.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.24.0",
    "esbuild": "^0.25.0",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
